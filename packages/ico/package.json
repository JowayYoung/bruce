		"chalk": "5.3.0",
		"chokidar": "3.6.0",
		"commander": "12.1.0",
		"figures": "6.1.0",
		"is-animated": "2.0.2",
		"mime-types": "2.1.35",
		"ora": "8.0.1",
		"semver": "7.6.2"
	},
	"devDependencies": {
		"@types/mime-types": "2.1.4",
		"@types/node": "20.14.9",
		"@types/semver": "7.5.8",
		"nodemon": "3.1.4",
		"typescript": "5.5.2"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
