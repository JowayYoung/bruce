{
	"name": "@yangzw/bruce-ico",
	"version": "1.1.7",
	"description": "An Icon Processing Tool For Generating Css Icons Based On Css Background And Mask",
	"keywords": [
		"bruce-ico",
		"ico",
		"css",
		"background",
		"mask"
	],
	"author": {
		"name": "JowayYoung",
		"email": "young.joway@outlook.com"
	},
	"homepage": "https://github.com/JowayYoung",
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/bruce.git"
	},
	"bugs": {
		"email": "young.joway@outlook.com",
		"url": "https://github.com/JowayYoung/bruce/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"bruce-ico": "dist/index.js"
	},
	"scripts": {
		"build": "rimraf dist && tsc --project tsconfig.json && npm link",
		"dev": "nodemon --watch src --watch package.json --watch tsconfig.json --ext ts --exec \"npm run build\"",
		"mount": "npm link"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.6.0"
	},
	"dependencies": {
		"@yangzw/bruce-us": "workspace:*",
		"chalk": "5.3.0",
		"chokidar": "3.5.3",
		"commander": "11.1.0",
		"figures": "6.0.1",
		"is-animated": "2.0.2",
		"mime-types": "2.1.35",
		"ora": "7.0.1",
		"semver": "7.5.4"
	},
	"devDependencies": {
		"@types/mime-types": "2.1.4",
		"@types/node": "20.10.4",
		"@types/semver": "7.5.6",
		"nodemon": "3.0.2",
		"typescript": "5.3.3"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}