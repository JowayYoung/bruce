{
	"name": "@yangzw/bruce-std",
	"version": "1.1.3",
	"description": "A VSCode Configuration Tool Integrating Stylelint And Eslint",
	"keywords": [
		"bruce-std",
		"std",
		"standard",
		"rule",
		"vscode",
		"lint",
		"stylelint",
		"eslint",
		"tslint",
		"typescript-eslint"
	],
	"author": {
		"name": "JowayYoung",
		"email": "young.joway@outlook.com"
	},
	"homepage": "https://github.com/JowayYoung",
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/bruce.git"
	},
	"bugs": {
		"email": "young.joway@outlook.com",
		"url": "https://github.com/JowayYoung/bruce/issues"
	},
	"main": "dist/index.mjs",
	"bin": {
		"bruce-std": "dist/index.mjs"
	},
	"scripts": {
		"build": "rimraf dist && tsc --project tsconfig.config.json && tsc --project tsconfig.json && npm link",
		"dev": "nodemon --watch config --watch src --watch package.json --watch tsconfig.json --ext ts,mts --exec \"npm run build\"",
		"mount": "npm link"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.10.0"
	},
	"dependencies": {
		"@babel/core": "7.23.2",
		"@babel/eslint-parser": "7.22.15",
		"@babel/preset-react": "7.22.15",
		"@typescript-eslint/eslint-plugin": "6.9.1",
		"@typescript-eslint/parser": "6.9.1",
		"@yangzw/bruce-us": "1.1.3",
		"chalk": "5.3.0",
		"clipboardy": "4.0.0",
		"commander": "11.1.0",
		"eslint": "8.52.0",
		"eslint-config-standard": "17.1.0",
		"eslint-config-standard-with-typescript": "39.1.1",
		"eslint-plugin-html": "7.1.0",
		"eslint-plugin-import": "2.29.0",
		"eslint-plugin-n": "16.2.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-vue": "9.18.1",
		"figures": "6.0.1",
		"ora": "7.0.1",
		"postcss": "8.4.31",
		"postcss-html": "1.5.0",
		"postcss-less": "6.0.0",
		"postcss-scss": "4.0.9",
		"semver": "7.5.4",
		"strip-json-comments": "5.0.1",
		"stylelint": "15.11.0",
		"stylelint-config-standard": "34.0.0",
		"stylelint-order": "6.0.3",
		"vue-eslint-parser": "9.3.2"
	},
	"devDependencies": {
		"@types/eslint": "8.44.6",
		"@types/node": "20.8.10",
		"@types/semver": "7.5.4",
		"nodemon": "3.0.1",
		"typescript": "5.2.2"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}