{
	"name": "@yangzw/bruce-pkg",
	"version": "1.1.6",
	"description": "A Package Processing Tool For Strengthening NPM Command",
	"keywords": [
		"bruce-pkg",
		"pkg",
		"master",
		"package",
		"create",
		"publish"
	],
	"author": {
		"name": "JowayYoung",
		"email": "young.joway@outlook.com"
	},
	"homepage": "https://github.com/JowayYoung",
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/bruce.git"
	},
	"bugs": {
		"email": "young.joway@outlook.com",
		"url": "https://github.com/JowayYoung/bruce/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"bruce-pkg": "dist/index.js"
	},
	"scripts": {
		"build": "rimraf dist && tsc --project tsconfig.json && npm link",
		"dev": "nodemon --watch src --watch package.json --watch tsconfig.json --ext ts --exec \"npm run build\"",
		"mount": "npm link"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.10.0"
	},
	"dependencies": {
		"@yangzw/bruce-us": "workspace:*",
		"chalk": "5.3.0",
		"commander": "11.1.0",
		"execa": "8.0.1",
		"figures": "6.0.1",
		"inquirer": "9.2.12",
		"listr2": "8.0.0",
		"ora": "7.0.1",
		"semver": "7.5.4"
	},
	"devDependencies": {
		"@types/inquirer": "9.0.7",
		"@types/node": "20.10.0",
		"@types/semver": "7.5.6",
		"nodemon": "3.0.1",
		"typescript": "5.3.2"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}