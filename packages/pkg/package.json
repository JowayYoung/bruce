{
	"name": "@yangzw/bruce-pkg",
	"version": "1.1.8",
	"description": "A package processing tool that enhances Npm commands",
	"keywords": [
		"bruce-pkg",
		"pkg",
		"package",
		"create",
		"publish"
	],
	"author": {
		"name": "JowayYoung",
		"email": "young.joway@outlook.com"
	},
	"homepage": "https://github.com/JowayYoung",
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/bruce.git"
	},
	"bugs": {
		"email": "young.joway@outlook.com",
		"url": "https://github.com/JowayYoung/bruce/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"bruce-pkg": "dist/index.js"
	},
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.json",
		"dev": "nodemon -w src -w package.json -w tsconfig.json -e ts -x \"npm run build\""
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.6.0"
	},
	"dependencies": {
		"@inquirer/prompts": "3.3.0",
		"@yangzw/bruce-us": "workspace:*",
		"chalk": "5.3.0",
		"commander": "11.1.0",
		"execa": "8.0.1",
		"figures": "6.0.1",
		"listr2": "8.0.1",
		"ora": "8.0.1",
		"semver": "7.5.4"
	},
	"devDependencies": {
		"@types/node": "20.10.5",
		"@types/semver": "7.5.6",
		"nodemon": "3.0.2",
		"rxjs": "7.8.1",
		"typescript": "5.3.3"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}